{
    "name": "rahban/laravel-logbook",
    "description": "A lightweight Laravel package to record, monitor, and manage API user journeys",
    "keywords": ["laravel", "logging", "api", "monitoring", "dashboard"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Rahban",
            "email": "muhammadrahban@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rahban\\LaravelLogbook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rahban\\LaravelLogbook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rahban\\LaravelLogbook\\Providers\\LogbookServiceProvider"
            ],
            "aliases": {
                "Logbook": "Rahban\\LaravelLogbook\\Facades\\Logbook"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}